pull:
    git pull
push:
    git add .
    git commit
    git push --repo=git@github.com:XDerekMaierX/$(pwd | sed 's/.*\///g').git -q
viewChanges:
    git log -p
wordCountPy:
    find -type f | grep ".py\|.js\|.css" | while read x ; do file "$x" ; done | grep ASCII | sed 's/:.*//g'  | while read x ; do cat "$x" ; done | grep -v "^$" | wc -l
forcePullMaster:
    git fetch --all
    git reset --hard origin/master
killPython:
    sudo kill $(ps -ef | grep "alpha\|omega\|lambda\|delta" | grep python -i | grep -v "grep" | awk '{print $2}')
startAll:
	# for developers
    job -y killPython || echo -n ""
    # xterm -geometry 70x10+0-0 -e sleep 2 &
    # xterm -geometry 70x10+0-150 -e sleep 2 &
    # xterm -geometry 70x10+0-300 -e sleep 2 &
    # xterm -geometry 70x10+0-450 -e sleep 2 &
    xterm -geometry 70x10+0-0 -e ./alpha/Alpha-Server.py &
    xterm -geometry 70x10+0-150 -e ./omega/Omega-Server.py &
    xterm -geometry 70x10+0-300 -e ./lambda-M/LambdaMaster-Server.py &
    xterm -geometry 70x10+0-450 -e ./delta/Delta-Database.py &
    # many minions
    ./lambda-m/LambdaMinion-Server.py &
    ./lambda-m/LambdaMinion-Server.py &
    ./lambda-m/LambdaMinion-Server.py &
    ./lambda-m/LambdaMinion-Server.py &
mpad:
    mousepad ./alpha/Alpha-Server.py ./lambdaUtils.py  ./omega/Omega-Server.py ./lambda-M/LambdaMaster-Server.py     ./delta/Delta-Database.py  ./lambda-m/LambdaMinion-Server.py &
mergeWithMaster:
	git checkout master
	git pull origin master
	git merge dev-1
	git push origin master
